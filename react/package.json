{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env.development -- vite",
    "staging": "cross-env NODE_ENV=staging dotenv -e .env.staging -- vite",
    "build": "vite build",
    "build:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- vite build",
    "build:staging": "cross-env NODE_ENV=staging dotenv -e .env.staging -- vite build",
    "lint": "eslint . --fix && prettier --write .",
    "lint:ci": "eslint . && prettier --check .",
    "preview": "cross-env NODE_ENV=staging dotenv -e .env.staging -- vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "tailwindcss": "^4.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^9.19.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "vite": "^6.1.0"
  }
}
